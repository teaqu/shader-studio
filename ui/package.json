{
  "name": "shader-studio-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "copy-to-extension": "rimraf ../extension/ui-dist && cpx \"dist/**/*\" ../extension/ui-dist",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.5.2",
    "svelte": "^5.28.1",
    "svelte-check": "^4.0.8"
  },
  "dependencies": {
    "@shader-studio/types": "*"
  }
}
