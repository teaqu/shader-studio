{
  "name": "shader-explorer-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "copy-to-extension": "rimraf ../extension/shader-explorer-dist && cpx \"dist/**/*\" ../extension/shader-explorer-dist",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.5.2",
    "cpx": "^1.5.0",
    "jsdom": "^25.0.1",
    "rimraf": "^6.0.1",
    "svelte": "^5.28.1",
    "svelte-check": "^4.0.8",
    "tslib": "^2.8.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@shader-studio/types": "*",
    "@shader-studio/rendering": "*"
  }
}
